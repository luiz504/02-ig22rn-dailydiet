{
  "expo": {
    "name": "Daily Diet",
    "slug": "dailydiet",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/daily-diet-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FAFAFA"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FAFAFA"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.luiz504.dailydiet"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "backgroundColor": "#FAFAFA",
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": ["expo-localization"],
    "extra": {
      "eas": {
        "projectId": "122338f9-816c-497c-8211-d61827b4aa2e"
      }
    }
  }
}
